@model IEnumerable<CRUDWithDapperORMD8.Models.Book>

@{
    ViewData["Title"] = "Index";
}


<div class="row">
    <div class="col-sm-6">
        <a asp-action="Create"><i class="fa fa-plus" style="font-size:28px;color:red"> Create New</i></a>
    </div>
    <div class="col-sm-6" style="display: flex; justify-content: flex-end">
        <h1 style="align-items: center;">Global Branch List</h1>
    </div>
</div>

<table class="ItemTranTable" border="1">
    <thead>
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Title)
                </td>
            </tr>
        }
    </tbody>
</table>